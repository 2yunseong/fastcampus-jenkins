pipeline {
    agent any
    environment {
        BRANCH_NAME = "$GIT_BRANCH"
    }
    stages {
        stage("ww") {
            steps {
                sh "printenv"
            }
        }

         stage("ww2") {
                    when {
                        branch "main"
                    }
                    steps {
                        sh "printenv"
                    }
                }
    }
}